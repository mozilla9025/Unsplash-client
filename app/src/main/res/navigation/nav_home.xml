<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_home"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="app.wallpaper.modules.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_photoDetailsFragment"
            app:destination="@id/photoDetailsFragment"
            app:popUpTo="@+id/homeFragment" />
    </fragment>

    <fragment
        android:id="@+id/photoDetailsFragment"
        android:name="app.wallpaper.modules.photo.PhotoFragment"
        android:label="PhotoDetailsFragment"
        tools:layout="@layout/fragment_photo">
        <argument
            android:name="photo"
            app:argType="app.wallpaper.domain.data.Photo"
            app:nullable="false" />
    </fragment>

</navigation>